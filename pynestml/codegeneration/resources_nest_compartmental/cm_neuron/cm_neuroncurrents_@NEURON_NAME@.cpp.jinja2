void nest::{{continuous_name}}{{cm_unique_suffix}}::f_numstep(const std::vector< double >& shared_v_comp, const long& lag {% for ode in continuous_info["Dependencies"]["concentrations"] %}, const std::vector< double >& shared_{{ode.lhs.name}}{% endfor %}{% if continuous_info["Dependencies"]["receptors"]|length %}
                        {% endif %}{% for inline in continuous_info["Dependencies"]["receptors"] %}, const std::vector< double >& shared_{{inline.variable_name}}{% endfor %}{% if continuous_info["Dependencies"]["channels"]|length %}
                        {% endif %}{% for inline in continuous_info["Dependencies"]["channels"] %}, const std::vector< double >& shared_{{inline.variable_name}}{% endfor %}{% if continuous_info["Dependencies"]["continuous"]|length %}
                        {% endif %}{% for inline in continuous_info["Dependencies"]["continuous"] %}, const std::vector< double >& shared_{{inline.variable_name}}{% endfor %}
                        , std::vector< double >& i_val, std::vector< double >& g_val)
{

    {% if "time_resolution_var" in continuous_info %}
    double {{ printer_no_origin.print(continuous_info["time_resolution_var"]) }} = Time::get_resolution().get_ms();
    {% endif %}

    {% for port_name, port_info in continuous_info["Continuous"].items() %}
    std::vector< double > {{ port_name }}(neuron_{{ continuous_name }}_continuous_input_count, 0.);
    {% endfor %}
    #pragma omp simd
    for(std::size_t i = 0; i < neuron_{{ continuous_name }}_continuous_input_count; i++){
        {% for port_name, port_info in continuous_info["Continuous"].items() %}
        {{ port_name }}[i] = {{ port_name }}_[i]->get_value( lag );
        {% endfor %}
    }

    {%- for function in continuous_info["Functions"] %}
    {{render_lambda(function, printer_no_origin)}}
    {%- endfor %}

  #pragma omp simd
  for(std::size_t i = 0; i < neuron_{{ continuous_name }}_continuous_input_count; i++){
            double v_comp = shared_v_comp[compartment_association[i]];
            {% for inline in receptor_info["Dependencies"]["concentrations"] %}
            double {{inline.variable_name}} = shared_{{inline.variable_name}}[compartment_association[i]];
            {% endfor %}
            {% for inline in receptor_info["Dependencies"]["receptors"] %}
            double {{inline.variable_name}} = shared_{{inline.variable_name}}[compartment_association[i]];
            {% endfor %}
            {% for inline in receptor_info["Dependencies"]["channels"] %}
            double {{inline.variable_name}} = shared_{{inline.variable_name}}[compartment_association[i]];
            {% endfor %}
            {% for inline in receptor_info["Dependencies"]["continuous"] %}
            double {{inline.variable_name}} = shared_{{inline.variable_name}}[compartment_association[i]];
            {% endfor %}

        {%- for replacement, expression in continuous_info["cse_function_replacements"].items() %}
            double {{ replacement }} = {{ vector_printer.print(expression, "i", non_vec_vars) }};
        {%- endfor %}
        {%- for replacement, expression in continuous_info["cse_body_replacements"].items() %}
            double {{ replacement }} = {{ vector_printer.print(expression, "i", non_vec_vars) }};
        {%- endfor %}

      {%- for convolution, convolution_info in continuous_info["convolutions"].items() %}
      {%- for state_variable_name, state_variable_info in convolution_info["analytic_solution"]["kernel_states"].items() %}
      {{state_variable_name}}[i] = {{ vector_printer.print(state_variable_info["update_expression"], "i", non_vec_vars) }};
      {{state_variable_name}}[i] += self_spikes * {{ vector_printer.print(state_variable_info["init_expression"], "i", non_vec_vars) }};
      {%- endfor %}
      {%- endfor %}

      //update ODE state variable
      {%- for ode_variable, ode_info in continuous_info["ODEs"].items() %}
          {%- for propagator, propagator_info in ode_info["transformed_solutions"][0]["propagators"].items() %}
      double {{ propagator }} = {{ vector_printer.print(propagator_info["init_expression"], "i", non_vec_vars) }};
          {%- endfor %}
          {%- for state, state_solution_info in ode_info["transformed_solutions"][0]["states"].items() %}
      {{state}}[i] = {{ vector_printer.print(state_solution_info["update_expression"], "i", non_vec_vars) }};
          {%- endfor %}
      {%- endfor %}

      // total current
      // this expression should be the transformed inline expression
      i_tot_{{continuous_name}}[i] = {{ vector_printer.print(continuous_info["root_expression"], "i", non_vec_vars) }};

      // derivative of that expression
      // voltage derivative of total current
      // compute derivative with respect to current with sympy
      double d_i_tot_dv = {{ vector_printer.print(continuous_info["inline_derivative"], "i", non_vec_vars) }};

      // for numerical integration
      g_val[i] = - d_i_tot_dv;
      i_val[i] = i_tot_{{continuous_name}}[i] - d_i_tot_dv * v_comp;
  }

  f_update();

    //update recordable inlines
    #pragma omp simd
    for(std::size_t i = 0; i < neuron_{{ continuous_name }}_continuous_input_count; i++){
    {%- for variable_info in continuous_info["SecondaryInlineExpressions"] %}
        {%- if variable_info.is_recordable %}
        {%- set variable = variable_info.variable_name %}
        {%- set rhs_expression = variable_info.expression %}
        {{ variable }}[i] = {{ vector_printer.print(rhs_expression, "i", non_vec_vars) }};
        {%- endif %}
    {%- endfor %}
    }
}

