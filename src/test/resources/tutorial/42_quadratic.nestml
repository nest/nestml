neuron quadratic_IaF:

  state:
    V_m mV = 0mV
  end

  equations:
    V_m' = I_syn + a * (V_m - V_reset)*(V_m - V_th)/1mV
  end

  parameters:
    I_syn pA = 10pA
    a real = 7.0
    V_reset mV =  10mV
    V_peak mV =  20mV
    V_th mV =  15mV
  end

  input:
    spikes nS <- spike
  end

  output: spike

  update:
    integrate_odes()
    if V_m > V_peak:
      V_m = V_reset
      emit_spike()
    end
  end


end