neuron NeuronInTest:
  state:
      x mV
      g_z mV
      g_z' mV/ms
      g_ex mV # this error is reported because of the shape g_ex
  end

  equations:
    function x mV = 1
    shape g_ex = t # error, g_z is already defined
    g_z'' = t
    g_z'' = t*2 # error, g_z' is defined by the last definition
  end

  parameters:
    x real = x * 2
    y real
  end

  input:
    y nS <- spike
  end

  equations:
    function x real = 12
  end
