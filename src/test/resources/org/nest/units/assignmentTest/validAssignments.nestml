neuron invalidAssignmentTest:
  state:
    Resist Ohm = 3 Ohm 
    Real real
    Integer integer
    String string
  end

  parameters:
    Current A = 3 A
    Potential V = 3 V
  end

  input:
      spikeInh nS <- inhibitory spike
      spikeExc nS  <- excitatory spike
      currents <- current
  end

  output: spike

  internals:
  end

  update:   
    V3 V**2 = (3V+5V)**2
    Resist = 8Ohm 

    Real *= 2.5
    Real *= 2
    Integer *= 2
    Resist *= 2.5
    Resist *= 2   

    Real /= 2.5
    Real /= 2
    Resist /= 2.5
    Resist /= 2   
    
    Real -= 2.5
    Real -= 2
    Integer -= 2
    Resist -= Resist

    String += Real
  end

end
