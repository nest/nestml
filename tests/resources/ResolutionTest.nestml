model resolution_test:
    state:
        test1 integer = 10

    parameters:
        test4 mS = 20mV
        test10 string = "test10"

    update:
        test6 boolean = true

    function test1(arg1 mV,arg2 integer):
        test2 integer = arg1 + arg2
        test3 mV = test2 + arg2
        if not test2 == test3:
            return test2
        else:
            return test3
