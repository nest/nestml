neuron resolution_test:
    state:
       test1 integer = 10
    end

    parameters:
        test4 mS = 20mV
        test10 string = "test10"
    end

    update:
        test6 boolean = true
    end

    function test1(arg1 mV,arg2 integer):
        test2 integer = arg1 + arg2
        test3 mV = test2 + arg2
        if not test2 == test3:
            return test2
        else:
            return test3
        end
    end
end
