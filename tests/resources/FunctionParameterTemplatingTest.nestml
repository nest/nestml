neuron templated_function_parameters_type_test:
    state:
       force N
       foo real
    end

    update:
        force = max(1 N, 10 N)
        force = max(1 A, 10 N)
        force = max(1 N, 10 A)
        force = max(1 A, 10 A)
        foo = max(1, 3.14)
    end
end
