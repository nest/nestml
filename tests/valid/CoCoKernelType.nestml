model CoCoKernelCorrectlyTyped:
    parameters:
        tau ms = 10 ms

    state: # variable is now defined in the initial block, thus everything is correct.
        g real = 1
        g' ms**-1 = 1 ms**-1

    equations:
        kernel g'' = -1/tau**2 * g - 2/tau * g'
