# delay_test_assigned_delay2_synapse
# ##################################
#
model delay_test_assigned_delay2_synapse:
  state:
    w real = 1    # Synaptic weight
    delay ms = 1 ms    # Synaptic transmission delay

  input:
    pre_spikes <- spike

  output:
    spike

  onReceive(pre_spikes):
    emit_spike(w)
