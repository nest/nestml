"""
delay_test_synapse_plastic
##########################
"""
model delay_test_plastic_synapse:
  state:
    x ms = 0 ms
    w real = 1    # Synaptic weight
    d ms = 1 ms    # Synaptic transmission delay

  input:
    pre_spikes <- spike

  output:
    spike(weight real, delay ms)

  onReceive(pre_spikes):
    emit_spike(w, d)

  update:
    x = d
