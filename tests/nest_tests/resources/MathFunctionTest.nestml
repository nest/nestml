model math_function_test:
    state:
        x real = 0.
        ln_state real = 0.
        log10_state real = 0.
        erf_state real = 0.
        erfc_state real = 0.
        ceil_state real = 0.
        floor_state real = 0.
        round_state real = 0.
        abs_state real = 0.

    update:
        ln_state = ln(x)
        log10_state = log10(x)
        erf_state = erf(x)
        erfc_state = erfc(x)
        ceil_state = ceil(x / 10.)
        floor_state = floor(x / 10.)
        round_state = round(x / 10.)
        abs_state = abs(x)
        x = x + 1.
