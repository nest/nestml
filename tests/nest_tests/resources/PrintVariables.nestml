neuron print_variable:
    state:
      V_m V = -50 mV
      V_thr MA*Ohm = -70 mV
    end

    initial_values:
      V_abs mV = 0 mV
    end

    update:
      V_abs = V_m
      println("print: This is a simple print statement")
      print("print: Membrane voltage: {V_m}, threshold: {V_thr}, and V_abs: {V_abs}")
    end
end
