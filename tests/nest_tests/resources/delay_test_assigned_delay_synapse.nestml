"""
delay_test_assigned_delay_synapse
#################################
"""
model delay_test_assigned_delay_synapse:
  state:
    w real = 1    # Synaptic weight
    d ms = 1 ms    # Synaptic transmission delay

  input:
    pre_spikes <- spike

  output:
    spike(weight real, delay ms)

  onReceive(pre_spikes):
    d = 2 ms       # not allowed!
    emit_spike(w, d)
