model string_handling_test:
    state:
        s1 string = "abc"
        s2 string = "def"
        b1 boolean = false
        b2 boolean = false

    parameters:
        s3 string = "ghi"
        s4 string = "klm"

    internals:
        s5 string = "ghi"

    update:
        s7 string = s1 + s2
        if s7 == "abcdef":
            b1 = true

        if s3 + s4 == s5 + "klm":
            b2 = true
